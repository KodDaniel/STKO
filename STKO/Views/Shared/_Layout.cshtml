<!DOCTYPE html>
<html lang="sv">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@(ViewBag.Title ?? "Studiekollen") </title>
    <link rel="shortcut icon" href="#">
    <link asp-href-include="/lib/bootstrap/css/*.min.css" asp-href-exclude="**/*-reboot*,**/*-grid*" rel="stylesheet" />
    <link asp-href-include="/css/site.css" rel="stylesheet" />
    <link rel="stylesheet" asp-href-include="/lib/font-awesome/css/*.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"
            asp-fallback-src="/scripts/jquery.min.js" asp-append-version="true"
            asp-fallback-test="window.jQuery">
    </script>
    <script asp-src-include="/lib/bootstrap/js/*.min.js" asp-append-version="true"></script>
</head>
<body>  
<vc:navbar-component />
<main role="main">
    <div class="container">
        <div class="row">
            <div class="col-md-1"></div>
            <div class="col-md-12">
                @RenderBody()
            </div>
            <div class="col-md-1"></div>
        </div>
    </div>
</main>
</body>
</html>